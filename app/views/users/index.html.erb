<h1>Listing users</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Email address</th>
    <th>Maintain parts</th>
    <th>Administrator</th>
    <th>Crypted password</th>
    <th>Salt</th>
    <th>Remember token</th>
    <th>Remember token expires at</th>
    <th>State</th>
    <th>Key timestamp</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.name %></td>
    <td><%= user.email_address %></td>
    <td><%= user.maintain_parts %></td>
    <td><%= user.administrator %></td>
    <td><%= user.crypted_password %></td>
    <td><%= user.salt %></td>
    <td><%= user.remember_token %></td>
    <td><%= user.remember_token_expires_at %></td>
    <td><%= user.state %></td>
    <td><%= user.key_timestamp %></td>
    <td><%= link_to 'Show', user %></td>
    <td><%= link_to 'Edit', edit_user_path(user) %></td>
    <td><%= link_to 'Destroy', user, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New User', new_user_path %>
